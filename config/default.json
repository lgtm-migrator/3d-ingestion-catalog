{
  "openapiConfig": {
    "filePath": "./bundledApi.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": true
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "db": {
    "type": "postgres",
    "host": "127.0.0.1",
    "port": 5432,
    "username": "postgres",
    "password": "postgres",
    "database": "3d-catalog-manager-dev",
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    }
  }
}
